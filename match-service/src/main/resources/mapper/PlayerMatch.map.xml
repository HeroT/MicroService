<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.peihao.nba.match.repository.PlayerMatchRepository">


    <select id="findOne" resultType="PlayerMatch">
        SELECT *
        FROM `player_data`
        WHERE `player_data`.`id` = #{id}
    </select>

</mapper>